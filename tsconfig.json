{
    "compilerOptions": {
        "module": "CommonJS",
        "alwaysStrict": true,
        "strict": true,
        "strictNullChecks": true,
        "allowUnusedLabels": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictPropertyInitialization": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
    },
    "exclude": [
        "node_modules"
    ]
}